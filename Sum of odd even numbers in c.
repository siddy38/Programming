#include <stdio.h>

int main() {
    int start, end;
    int evenSum = 0, oddSum = 0;

    printf("Enter the start of the range: ");
    scanf("%d", &start);

    printf("Enter the end of the range: ");
    scanf("%d", &end);

    
    for (int i = start; i <= end; i++) {
        if (i % 2 == 0) {
            evenSum += i;  
        } else {
            oddSum += i;  
        }
    }

   
    printf("Sum of even numbers: %d\n", evenSum);
    printf("Sum of odd numbers: %d\n", oddSum);

    return 0;
}
